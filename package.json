{
  "name": "swc-editor",
  "author": "jeffery",
  "private": true,
  "version": "0.1.1",
  "keywords": [
    "editor",
    "ai",
    "ai editor"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build:bold": "pnpm --filter @wc-editor/extension-bold run build",
    "dev:editor": "pnpm --filter @wc-editor/core run dev",
    "docs": "cross-env APP_BASE_PATH=/Ai-Block-Editor/ vitepress dev docs --port=8732",
    "build:docs": "vitepress build docs",
    "build:github": "cross-env APP_BASE_PATH=/Ai-Block-Editor/ npm run build:docs",
    "build": "vite build",
    "preview": "vite preview",
    "changeset": "changeset",
    "update-changelog": "changeset version",
    "release": "changeset publish --registry=https://registry.npmjs.com/ --access=public",
    "commit": "cz",
    "preinstall": "npx only-allow pnpm",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@types/node": "^20.12.7",
    "@wc-editor/eslint-config": "workspace:^",
    "@wc-editor/typescript-config": "workspace:^",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "omi": "^7.6.7",
    "only-allow": "^1.2.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^2.41.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-node": "^10.9.1",
    "turbo": "^1.13.2",
    "typescript": "^5.4.5",
    "vite": "^4.5.3"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "^2.15.0",
    "crypto-js": "^4.2.0",
    "i18next": "^23.7.6",
    "lodash": "^4.17.21",
    "tippy.js": "^6.3.7"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --ext .js,.jsx,.vue,.ts,.tsx --fix --quiet ./"
    ]
  }
}
